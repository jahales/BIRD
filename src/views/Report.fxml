<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>


<HBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReportController">
	<children>
		<VBox alignment="TOP_LEFT" prefHeight="400" prefWidth="200" spacing="5">
			<children>
				<Label text="X Axis" />
				<ChoiceBox fx:id="xAxisChoices"/>
				<Label text="Y Axis" />
				<ListView fx:id="yAxisChoices" prefHeight="123" />
			</children>
			<padding>
				<Insets bottom="5" left="5" right="5" top="5" />
			</padding>
		</VBox>
		<LineChart fx:id="graph" title="Results">
			<xAxis>
				<NumberAxis label="Time" side="BOTTOM" fx:id="xAxis" />
			</xAxis>
			<yAxis>
				<NumberAxis fx:id="yAxis" label="Speed" side="LEFT" />
			</yAxis>
		</LineChart>
	</children>
</HBox>