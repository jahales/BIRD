<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.parts.ConicalFrustumController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Upper Diameter" GridPane.rowIndex="1" />
        <Label text="Lower Diameter" GridPane.rowIndex="2" />
        <Label text="Value" GridPane.columnIndex="1" />
        <Label text="Error" GridPane.columnIndex="2" />
        <Label text="Units" GridPane.columnIndex="3" />
        <TextField fx:id="upperDiameterValue" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <TextField fx:id="upperDiameterError" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <fx:include fx:id="upperDiameterUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <TextField fx:id="lowerDiameterValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="lowerDiameterError" GridPane.columnIndex="2" GridPane.rowIndex="2" />
        <fx:include fx:id="lowerDiameterUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="2" />
    </children>
</GridPane>
