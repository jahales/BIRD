<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.parts.NoseConeController" >
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Shape Parameter" GridPane.rowIndex="1" />
		<Label text="Diameter" GridPane.rowIndex="2" />
		<Label text="Nose Shape" GridPane.rowIndex="3" />
		
		<Label text="Value" GridPane.columnIndex="1" />
		<TextField fx:id="shapeParameter" GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<TextField fx:id="diameterValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
		
		<Label text="Error" GridPane.columnIndex="2" />
		<TextField fx:id="diameterError" GridPane.columnIndex="2" GridPane.rowIndex="2" />
		
		<Label text="Units" GridPane.columnIndex="3" />
		<fx:include fx:id="diameterUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="2" />
		<fx:include fx:id="shape" source="fxmlpieces/Shape.fxml" GridPane.columnIndex="3" GridPane.rowIndex="3" />
	</children>
</GridPane>
