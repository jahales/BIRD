<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<columnConstraints>
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
		<ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="100" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
		<RowConstraints minHeight="10" prefHeight="30" vgrow="SOMETIMES" />
	</rowConstraints>
	<children>
		<Label text="Value" GridPane.columnIndex="1" />
		<Label text="Error" GridPane.columnIndex="2" />
		<Label text="Units" GridPane.columnIndex="3" />
		<Label text="Name" GridPane.rowIndex="1" />
		<TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1" />
		<Label text="Mass" GridPane.rowIndex="2" />
		<TextField fx:id="massValue" GridPane.columnIndex="1" GridPane.rowIndex="2" />
		<TextField fx:id="massError" GridPane.columnIndex="2" GridPane.rowIndex="2" />
		<fx:include fx:id="massUnits" source="fxmlpieces/Mass.fxml" GridPane.columnIndex="3" GridPane.rowIndex="2" />
		<Label text="Axial Length" GridPane.rowIndex="3" />
		<TextField fx:id="axialLengthValue" GridPane.columnIndex="1" GridPane.rowIndex="3" />
		<TextField fx:id="axialLengthError" GridPane.columnIndex="2" GridPane.rowIndex="3" />
		<fx:include fx:id="AxialLengthUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="3" />
		<Label text="Axial Offset" GridPane.rowIndex="4" />
		<TextField fx:id="AxialOffsetValue" GridPane.columnIndex="1" GridPane.rowIndex="4" />
		<TextField fx:id="axialOffsetError" GridPane.columnIndex="2" GridPane.rowIndex="4" />
		<fx:include fx:id="axialOffsetUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="4" />
		<Label text="Radial Offset" GridPane.rowIndex="5" />
		<TextField fx:id="radialOffsetValue" GridPane.columnIndex="1" GridPane.rowIndex="5" />
		<TextField fx:id="radialOffsetError" GridPane.columnIndex="2" GridPane.rowIndex="5" />
		<fx:include fx:id="radialOffsetUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="5" />
		<Label text="Thickness" GridPane.rowIndex="6" />
		<TextField fx:id="thicknessValue" GridPane.columnIndex="1" GridPane.rowIndex="6" />
		<TextField fx:id="thicknessError" GridPane.columnIndex="2" GridPane.rowIndex="6" />
		<fx:include fx:id="thicknessUnits" source="fxmlpieces/Length.fxml" GridPane.columnIndex="3" GridPane.rowIndex="6" />
	</children>
</GridPane>